<a href="/dashboard" style="display: inline;"><< Dashboard</a>

<div class="admin-title">
    <h2>Authorized Information</h2>
</div>

<ul class="admin-posts">
    <% data.forEach(user=>{ %>

        <li>
            <p>
                <%= user.username%>
            </p>
            <p>
                Phone: <%= user.phone%>
            </p>
            <p>
                Email: <%= user.email%>
            </p>
            <div class="admin-post-controls">
                <a href="/other-logs/<%= user.username %>" class="btn">Transactions</a>
            </div>
        </li>

    <%})%>
</ul>